{
  "name": "Tidy Task",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128" : "logo128.png"
  },
  "background" : {
    "service-worker" : "service-worker.js",
    "type" : "module"
  },
  "action": {
    "default_icon": "logo128.png",
    "default_title": "Tidy Task"
  },
  "permissions" : [
    "scripting"
  ],
  "host_permissions" : [
    "http://*/*",
    "https://*/*"
  ]
}
