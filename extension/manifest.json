{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "icon": {
    "128" : "logo128.png"
  },
  "background" : {
    "service-worker" : "service-worker.js",
    "type" : "module"
  },
  "action": {
    "default_popup": "hello.html",
    "default_icon": "logo128.png"
  },
  "permissions" : [
    "scripting"
  ],
  "host_permissions" : [
    "http://*/*",
    "https://*/*"
  ]
}
